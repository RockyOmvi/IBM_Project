                        <div class="text-sm text-neutral-500">Results</div>
                    </div>
                </div>
            </div>

            <!-- SEARCH PREVIEW MOCKUP -->
            <div class="relative opacity-0 animate-fade-right delay-300">
                <div class="absolute -inset-1 bg-gradient-to-r rounded-2xl blur opacity-20 from-blue-600 to-purple-600">
                </div>
                <div
                    class="relative border backdrop-blur-xl rounded-2xl shadow-2xl overflow-hidden bg-neutral-900/90 border-neutral-800/80">
                    <!-- HEADER -->
                    <div class="px-6 py-4 border-b flex items-center bg-neutral-900/80 border-neutral-800">
                        <div class="flex items-center space-x-2">
                            <span class="inline-block w-3 h-3 rounded-full bg-red-500"></span>
                            <span class="inline-block w-3 h-3 rounded-full bg-yellow-500"></span>
                            <span class="inline-block w-3 h-3 rounded-full bg-green-500"></span>
                        </div>
                        <div class="ml-6 flex items-center gap-2">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
